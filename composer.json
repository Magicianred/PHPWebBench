{
    "name": "ezimuel/phpwebbench",
    "description": "PHP Web Bench is a tool for benchmarking web application",
    "license": "BSD-3-Clause",
    "keywords": [
        "benchmark",
        "http",
        "php"
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-console": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4.4 || ^5.7.14"
    },
    "autoload": {
        "psr-4": {
            "PHPWebBench\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPWebBenchTest\\": "test/"
        }
    },
    "bin" : [ "bin/runbench" ]
}
